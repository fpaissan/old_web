<script>
function toggle() {
  var x = document.getElementById("toggle-div");
  var btn = document.getElementById("thebutton");

  if (x.style.display === "block") {
    x.style.display = "none";
    btn.innerHTML = "+ Show more..."
  } else {
    x.style.display = "block";
    btn.innerHTML = "Hide"
  }
}

</script>
